{
  "name": "plagiarise",
  "version": "0.0.1",
  "description": "https://en.wikipedia.org/wiki/Plagiarism",
  "main": "./server/build/main.js",
  "scripts": {
    "build": "npm run build_clear && npm run build_client && npm run build_server",
    "build_clear": "rm -rf ./client/distribute/source && rm -rf ./server/build",
    "build_client": "./node_modules/webpack/bin/webpack.js --config client/webpack.config.js",
    "build_server": "tsc -p server/tsconfig.json",
    "iterate": "npm run lint && npm run build && node --version && npm run start",
    "lint": "yarn run lint_tslint && yarn run lint_prettier",
    "lint_prettier": "./node_modules/prettier/bin-prettier.js {client,server}/source/**/* --check --write",
    "lint_tslint": "./node_modules/tslint/bin/tslint {client,server}/source/**/*.{ts,tsx} --fix --force",
    "postinstall": "npm run build",
    "start": "node .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.2",
    "@types/html-to-text": "^1.4.31",
    "@types/lodash": "^4.14.146",
    "@types/node": "^12.12.7",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/request-promise": "^4.1.44",
    "bluebird": "^3.7.1",
    "chalk": "^3.0.0",
    "express": "^4.17.1",
    "html-to-text": "^5.1.1",
    "keyword-extractor": "0.0.18",
    "lodash": "^4.17.15",
    "prettier": "^1.18.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.5",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "wikiapi": "^1.7.0"
  },
  "engines": {
    "node": "^12.11.1"
  }
}
